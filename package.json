{
  "name": "qwiki-cite",
  "version": "0.0.1",
  "description": "Instantly generate a Wikipedia citation template for the current website",
  "license": " GPL-3.0-only",
  "dependencies": {
    "parcel": "^2.10.3",
    "typescript": "^5.3.3",
    "web-ext": "^7.9.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.10.3",
    "@types/webextension-polyfill": "^0.10.7",
    "chai": "^4.3.10",
    "mocha": "^10.2.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "parcel build src/manifest.json --config @parcel/config-webextension",
    "watch": "parcel watch src/manifest.json --config @parcel/config-webextension"
  }
}
